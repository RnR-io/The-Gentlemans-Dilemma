{
  "version": 2,
  "builds": [
    {
      "src": "analyze.js",
      "use": "@vercel/node"
    },
    {
      "src": "./**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "/analyze.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
